</div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  data() {\n    return {\n      list: [],\n      status: 0 // 默认为0 即默认排序 1 为升序 2 为降序\n    }\n  },\n  computed: {\n    new_priceList() {\n      if (!this.status) return this.list\n      return this.list.sort((a, b) => {\n        return this.status === 1 ? a.price - b.price : b.price - a.price\n      })\n    }\n  },\n  methods: {\n    initData() {\n      axios\n        .get(\n          'https://easy-mock.com/mock/5e1aa4ff7f109b0caa4d2e26/learnvue/commodity'\n        )\n        .then(res => {\n          let data = res.data.data\n          console.log(data, 'data')\n          this.list = data.list\n        })\n    }\n  },\n  created() {\n    setTimeout(() => {\n      this.initData()\n    }, 1000)\n  },\n  mounted() {},\n  updated() {},\n  destroyed() {}\n}\n</script>\n\n<style scoped>\n.commodity {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  justify-content: flex-start;\n}\n.item {\n  margin: 10px;\n  width: 200px;\n}\n.item_img {\n  width: 100px;\n  height: 100px;\n}\n</style>\n"]}]}